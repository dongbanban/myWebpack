{
    "presets": [
        [
            "@babel/preset-env",
            {
                // "targets": ">0.1%, not dead",
                "useBuiltIns": false
                // "corejs": {
                //     "version": 3,
                //     "proposals": true
                // }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", {
            "legacy": true
        }],
        ["@babel/plugin-proposal-private-methods", {
            "loose": true
        }],
        "@babel/plugin-transform-runtime"
    ]
}