{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": false
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", {
            "legacy": true
        }],
        ["@babel/plugin-proposal-private-methods", {
            "loose": true
        }],
        ["@babel/plugin-transform-runtime", { "corejs": 3, "proposals":true }]
    ]
}